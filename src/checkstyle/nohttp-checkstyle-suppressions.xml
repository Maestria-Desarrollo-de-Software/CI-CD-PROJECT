<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
	"-//Checkstyle//DTD SuppressionFilter Configuration 1.3//EN"
	"https://checkstyle.org/dtds/suppressions_1_3.dtd">
<suppressions>
	<suppress files="node_modules[\\/].*" checks=".*"/>
	<suppress files="node[\\/].*" checks=".*"/>

	<!-- outputs generados -->
	<suppress files="build[\\/].*" checks=".*"/>
	<suppress files="target[\\/].*" checks=".*"/>
	<suppress files="out[\\/].*" checks=".*"/>

	<!-- SBOM generado por CycloneDX -->
	<suppress files=".*application\.cdx\.json" checks="NoHttp"/>

	<!-- assets: fonts svg suelen traer DTD http:// histÃ³rico -->
	<suppress files=".*fonts[\\/].*\.svg" checks="NoHttp"/>

	<!-- binarios / recursos -->
	<suppress files=".+\.(jar|git|ico|p12|gif|jks|jpg|jpeg|png|svg|log)" checks="NoHttp"/>
</suppressions>
